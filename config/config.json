{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "dashboard-list-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/dashboardList/DashboardListWebPart.js",
          "manifest": "./src/webparts/dashboardList/DashboardListWebPart.manifest.json"
        }
      ]
    },
    "approver-edit-form-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/approverEditForm/ApproverEditFormWebPart.js",
          "manifest": "./src/webparts/approverEditForm/ApproverEditFormWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": {
      "path": "https://cdnjs.cloudflare.com/ajax/libs/jquery/3.5.0/jquery.min.js",
      "globalName": "jQuery"
    }
  },
  "localizedResources": {
    "DashboardListWebPartStrings": "lib/webparts/dashboardList/loc/{locale}.js",
    "ApproverEditFormWebPartStrings": "lib/webparts/approverEditForm/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js"
  }
}